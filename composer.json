{
    "name": "zepson-technologies/laravel-zepsonsms",
    "description": "This is my package laravel-zepsonsms",
    "keywords": [
        "Zepson-Technologies",
        "laravel",
        "laravel-zepsonsms"
    ],
    "homepage": "https://github.com/zepson-technologies/laravel-zepsonsms",
    "license": "MIT",
    "authors": [
        {
            "name": "Alpha Olomi",
            "email": "alphaolomi@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "spatie/laravel-package-tools": "^1.4.3",
        "illuminate/contracts": "^8.37"
    },
    "require-dev": {
        "brianium/paratest": "^6.2",
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15",
        "phpunit/phpunit": "^9.3",
        "spatie/laravel-ray": "^1.23",
        "vimeo/psalm": "^4.8"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\NextSms\\": "src",
            "NotificationChannels\\NextSms\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\NextSms\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "./vendor/bin/testbench package:test --parallel --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "NotificationChannels\\NextSms\\NextSmsServiceProvider"
            ],
            "aliases": {
                "NextSms": "NotificationChannels\\NextSms\\NextSmsFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
